procedure main {
    list <- [478,234,1,52,19,20,33,52,32,21,6,7,7,8,9];
    invoke insertionSort
};

procedure insertionSort {
    listResult <- [];
    while (len(list) > 0){
        invoke getIndexLargestNum;
        invoke removeLargestFromList
    };
    list <- listResult
};
 
procedure getIndexLargestNum {
    index = 0;
    size = len(list);
    for(i = index; i < size; i = i + 1){
        if(list[i] > list[index]){
            index = i
        }
    }
};

procedure removeLargestFromList {
    listAux <- [];
    size = len(list);
    for(i = 0; i < size; i = i + 1){
        if(i == index){
            listResult <- cons(list[0],listResult)
        } else {
            listAux <- cons(list[0],listAux)
        };
        list <- tail(list)
    };
    list <- listAux
};
